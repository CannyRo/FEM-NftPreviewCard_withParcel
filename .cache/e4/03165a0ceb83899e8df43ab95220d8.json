0px, md 768px, lg 1024px, xl 1280px, 2xl 1536px\r\n$breakpoints-up: (\r\n    'small': '40em',\r\n    'medium': '48em',\r\n    'large': '64em',\r\n    'xlarge': '80em',\r\n    'xxlarge': '96em',\r\n);\r\n\r\n// sm 639px, md 767px, lg 1023px, xl 1279px, 2xl 1535px\r\n$breakpoints-down : (\r\n    'xsmall' : '39.9375em',\r\n    'small': '47.9375em',\r\n    'medium': '63.9375em',\r\n    'large': '79.9375em',\r\n    'xlarge': '95.9375em',\r\n);\r\n\r\n@mixin breakpoint($size){\r\n    @media (min-width: map-get($breakpoints-up, $size)) {\r\n        @content;\r\n    }\r\n}\r\n\r\n@mixin breakpoint-down($size){\r\n    @media (max-width: map-get($breakpoints-down, $size)) {\r\n        @content;\r\n    }\r\n}\r\n","src\\scss\\util\\functions.scss":"/* https://css-tricks.com/snippets/sass/px-to-em-functions/ */\r\n@use \"sass:math\";\r\n\r\n@function rem($pixels, $context: 16) {\r\n    @return (math.div($pixels, $context)) * 1rem;\r\n}","src\\scss\\util\\colors.scss":"\r\n/* ___ Primary ___ */\r\n$softblue : hsl(215, 51%, 70%);\r\n$cyan : hsl(178, 100%, 50%);\r\n/* ___ Neutral ___ */\r\n$main-bg : hsl(217, 54%, 11%);\r\n$card-bg : hsl(216, 50%, 16%);\r\n$line-bg : hsl(215, 32%, 27%);\r\n$white : hsl(0, 0%, 100%);\r\n\r\n","src\\scss\\style.scss":"// Imports, Use, Forward, ...\r\n@use './util/minreset.scss';\r\n@use './util' as *;\r\n\r\n// Règle d'écriture d'une classe :\r\n// 1) Box\r\n// 2) Border\r\n// 3) Background\r\n// 4) Text\r\n// 5) Other\r\n\r\n// Règle BEM\r\n// Block = card\r\n// Element = image, title, describe, devise, value, date, avatar, autor\r\n// Modifier = here isn't modifier\r\n\r\n// ___Base___\r\nbody{\r\n    height: 100vh;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background-color: $main-bg;\r\n    font-family: $font-outfit;\r\n    font-size: rem(18);\r\n}\r\nfooter{\r\n    margin-top: auto;\r\n    text-align: center;\r\n    font-size: rem(11);\r\n    color: $white;\r\n}\r\na{\r\n    text-decoration: none;\r\n}\r\na:visited{\r\n    color: inherit;\r\n}\r\n\r\n// ___Layout___\r\n.card{\r\n    width: rem(327);\r\n    height: rem(543);\r\n    justify-self: center;\r\n    align-self: center;\r\n    margin-top: rem(62);\r\n\r\n    border-radius: rem(15);\r\n\r\n    background-color: $card-bg;\r\n\r\n    overflow: hidden;\r\n    &__header {\r\n        width: rem(278);\r\n        height: rem(278);\r\n        margin: rem(24);\r\n\r\n        border-radius: rem(15);\r\n\r\n        overflow: hidden;\r\n    }\r\n    &__content {\r\n        width: rem(278);\r\n        height: auto;\r\n        margin-left: rem(24);\r\n        margin-top: rem(24);\r\n        margin-right: rem(24);\r\n        margin-bottom: rem(16);\r\n    }\r\n    &__title {\r\n        margin-bottom: rem(12);\r\n        color: $white;\r\n        font-weight: 600;\r\n        font-style: normal;\r\n        font-size: rem(22);\r\n        line-height: rem(28);\r\n    }\r\n    &__describe {\r\n        margin-bottom: rem(16);\r\n        color: $softblue;\r\n        font-weight: 300;\r\n        font-style: normal;\r\n        font-size: rem(18);\r\n        line-height: rem(26);\r\n    }\r\n    &__value {\r\n        box-sizing: content-box;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        height: rem(19);\r\n        padding-bottom: rem(16);\r\n        border-bottom: rem(2) solid $line-bg;\r\n    }\r\n    &__footer {\r\n        width: rem(278);\r\n        height: auto;\r\n    }\r\n    &__autor {\r\n        display: flex;\r\n        align-items: center;\r\n        width: rem(278);\r\n        height: rem(33);\r\n        margin-left: rem(24);\r\n        margin-right: rem(24);\r\n        margin-bottom: rem(24);\r\n    }\r\n}\r\n// ___Module___\r\n.value{\r\ndisplay: flex;\r\ncolor: $cyan;\r\nfont-weight: 600;\r\nfont-style: normal;\r\nfont-size: rem(15);\r\nline-height: rem(19);\r\n}\r\n.value img{\r\n    width: auto;\r\n    height: 100%;\r\n    margin-right: rem(6.48);\r\n}\r\n.date{\r\n    display: flex;\r\n    color: $softblue;\r\n    font-weight: 400;\r\n    font-style: normal;\r\n    font-size: rem(15);\r\n    line-height: rem(19);\r\n}\r\n.date img{\r\n    width: auto;\r\n    height: 100%;\r\n    margin-right: rem(7.7);\r\n}\r\n.card__autor img{\r\n    width: auto;\r\n    height: 100%;\r\n    margin-right: rem(16);\r\n    border: rem(1) solid $white;\r\n    border-radius: 100%;\r\n}\r\n.card__autor p{\r\n    color: $softblue;\r\n    font-weight: 400;\r\n    font-style: normal;\r\n    font-size: rem(15);\r\n    line-height: rem(19);\r\n}\r\n.card__autor a{\r\n    color: $white;\r\n    margin-left: rem(5);\r\n}\r\n// ___State___\r\n\r\n// ___Theme___\r\n.attribution{\r\n    color: hsl(228, 45%, 44%);\r\n}\r\n.attribution:visited{\r\n    color: hsl(228, 45%, 44%);\r\n}","src\\scss\\util\\fonts.scss":"$font-outfit: 'Outfit', sans-serif;\r\n"},"lineCount":null}},"error":null,"hash":"36f65804759875226aebaa19dadf8c0c","cacheData":{"env":{}}}